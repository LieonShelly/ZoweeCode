//  墙体信息

#import <Foundation/Foundation.h>

@interface ZEWallInfo : NSObject<NSCoding>



/**
 *  String
 墙体编号
 */
@property(nonatomic,copy) NSString * WallNo;


/**
 *  Double
 墙体起点X
 */
@property(nonatomic,strong) NSNumber * WSX;

/**
 *  Double
 墙体起点Y
 */
@property(nonatomic,strong) NSNumber * WSY;

/**
 *  Double
 墙体终点X
 */
@property(nonatomic,strong) NSNumber * WEX;

/**
 *  Double
 墙体终点Y
 */
@property(nonatomic,strong) NSNumber * WEY;

/**
 *  	Double	墙角起点X
 */
@property(nonatomic,strong) NSNumber *WJSX;
/**
 *  Double	墙角起点Y
 */
@property(nonatomic,strong) NSNumber *WJSY;
/**
 *  	Double	墙角终点X
 */
@property(nonatomic,strong) NSNumber *WJEX;
/**
 *  	Double	墙角终点Y

 */
@property(nonatomic,strong) NSNumber *WJEY;

/**
 *  String
 上一段墙体编号
 */
@property(nonatomic,copy) NSString * WPreNo;

/**
 *  String
 下一段墙体编号
 */
@property(nonatomic,copy) NSString * WNextNo;

/**
 *  Double
 墙体厚度
 */
@property(nonatomic,strong) NSNumber * WThick;

/**
 *  Double
 墙体长度
 */
@property(nonatomic,strong) NSNumber * WLength;

/**
 *  Integer
 墙体是否可见（0 不1可见）
 */
@property(nonatomic,strong) NSNumber *WVisble;

/**
 *  Integer
 墙体上门数量
 */
@property(nonatomic,strong) NSNumber * WDoorCount;

/**
 *  Integer
 墙体上窗子数量
 */
@property(nonatomic,strong) NSNumber * WWindowsCount;

/**
 *  TLIST
 墙体上窗子、门列表
 */
@property(nonatomic,strong)NSMutableArray * WSubList;

@end
